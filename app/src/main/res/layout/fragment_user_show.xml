<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="org.ieeeguc.ieeeguc.fragments.user.UserShow">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="70dp"
            android:layout_marginStart="70dp"
            android:layout_marginTop="68dp"
            android:id="@+id/type" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/type"
            android:layout_alignLeft="@+id/type"
            android:layout_alignStart="@+id/type"
            android:layout_marginTop="48dp"
            android:id="@+id/id" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/id"
            android:layout_alignLeft="@+id/id"
            android:layout_alignStart="@+id/id"
            android:layout_marginTop="45dp"
            android:id="@+id/gender" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/gender"
            android:layout_alignLeft="@+id/gender"
            android:layout_alignStart="@+id/gender"
            android:layout_marginTop="52dp"
            android:id="@+id/bdate" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/bdate"
            android:layout_alignLeft="@+id/bdate"
            android:layout_alignStart="@+id/bdate"
            android:layout_marginTop="47dp"
            android:id="@+id/email" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/email"
            android:layout_alignLeft="@+id/email"
            android:layout_alignStart="@+id/email"
            android:layout_marginTop="44dp"
            android:id="@+id/phone" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/phone"
            android:layout_alignLeft="@+id/phone"
            android:layout_alignStart="@+id/phone"
            android:layout_marginTop="41dp"
            android:id="@+id/committee_id" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/committee_id"
            android:layout_alignLeft="@+id/committee_id"
            android:layout_alignStart="@+id/committee_id"
            android:layout_marginTop="37dp"
            android:id="@+id/committee_name" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignLeft="@+id/type"
            android:layout_alignStart="@+id/type"
            android:layout_marginTop="27dp"
            android:id="@+id/name" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignLeft="@+id/committee_name"
            android:layout_alignStart="@+id/committee_name"
            android:id="@+id/ieeemsh_id" />
    </RelativeLayout>

</FrameLayout>
